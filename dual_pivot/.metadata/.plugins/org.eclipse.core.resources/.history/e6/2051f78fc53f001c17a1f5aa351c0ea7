package vladimir;

import java.util.Random;
import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
//		System.out.print("랜덤 배열 수 입력");
		int n = 10;
//				sc.nextInt();
		
		int[] arr = new int[n];
		Random r = new Random();
		for (int a : arr) {
			a = r.nextInt() % n;
		}
		System.out.print("[");
		for (int a : arr) {
			System.out.print(a + ", ");
		}
		System.out.print("]");
		
		QuickSort qs = new QuickSort();
		qs.quickSort(arr, 0, n - 1);
		
		System.out.print("[");
		for (int a : arr) {
			System.out.print(" "+a);
		}
		System.out.print("]");
		
		sc.close();
	}
}
